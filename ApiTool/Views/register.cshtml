@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<dynamic>
@{
    Layout = "Shared/Master.cshtml";
    ViewBag["Title"] = "Register call"; 
}
    @Html.Partial("Shared/_partnerDetails", Model)

   <form action="/register/invoke" method="POST">
    @Html.Partial("Shared/_apiSelection", @Model.ApiVersion)
       
        <fieldset>
            <label for="email">Email address</label>
            <input type="text" name="email" class="long" value="auto_user1@opentable.com">
        </fieldset>

       <div class="buttons">
        <input type="submit" class="btn btn-go" value="Get results">
        <a href="/register" class="start-again">Start again</a>
    </div>
</form>

    @Html.Partial("Shared/_output", Model)
    @Html.Partial("Shared/_js", "Register")